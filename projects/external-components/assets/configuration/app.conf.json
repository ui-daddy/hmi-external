    "hmiAppConfiguration": {
        "ifPageNotFound": "login",
        "title": "UI Daddy",
        "baseUrl": "https://upms-api.azurewebsites.net",
        "metaTags": [
            {
                "name": "keyword",
                "content": "uidaddy"
            }
        ],
        "apiConfigurations": [
            {
                "name": "getSession",
                "url": "/rest/session",
                "method": "GET",
                "isApplicationSessionAPI": true,
                "onSuccess": {
                    "sharedData": [
                        {
                            "varName": "authenticated",
                            "staticData": "true",
                            "cache": true
                        }
                    ]
                }
            }
        ],
        "interceptorConfigurations": [
            {
                "statusCode": "401",
                "messageCode": "",
                "messageCodeResponseAccessor": "messageCode",
                "type": "error",
                "msgResponseAccessor": "message",
                "message": "Session Error",
                "pageUrl": "login",
                "clearSharedData": [
                    "authenticated",
                    "name"
                ]
            }
        ],
        "linkTags": [],
        "layoutProperties": {
            "css": {
                "configuration": {
                    "selectorName": "",
                    "xs": {
                        "background-color": "#f1f3f6"
                    },
                    "sm": {
                        "background-color": "#f1f3f6"
                    },
                    "md": {
                        "background-color": "#f1f3f6"
                    },
                    "lg": {
                        "background-color": "#f1f3f6"
                    }
                }
            }
        }
    },
