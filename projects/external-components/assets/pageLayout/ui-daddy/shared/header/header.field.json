{
    "header-dashboard": [
        {
            "altText": "UI Daddy",
            "baseProperties": {
                "name": "menu",
                "type": "staticImage",
                "id": "dnd-1"
            },
            "imageUrl": "assets/images/ui-daddy-logo.svg",
            "events": [
                {
                    "event": "click",
                    "actions": [
                        {
                            "actionType": "navigate",
                            "url": "/dashboard"
                            
                        }
                    ]
                }
            ],
            "layoutProperties": {
                "css": {
                    "configuration": [{
                        "selectorName": ".header__logo",
                        "xs": {
                            "justify-content": "flex-start",
                            "padding": "8px"
                        }
                    },
                    {
                        "selectorName": ".header__logo .dynamic-style-child",
                        "xs": {
                            "min-height": "40px",
                            "font-size": "30px",
                            "margin-left": "15px",
                            "color": "white",
                            "text-decoration": "none",
                            "cursor": "pointer"
                        }
                    }],
                    "customClassName": "header__logo"
                }
            }
        },
        {
            "baseProperties": {
                "name": "userName",
                "type": "staticText",
                "id": "dnd-2"
            },
            "value": "",
            "dynamicValueAccessor": [
                "name"
            ],
            "layoutProperties": {
                "css": {
                    "configuration": {
                        "selectorName": ".header__user-name",
                        "xs": {
                            "right": "40px",
                            "justify-content": "flex-end",
                            "color": "#64748b"
                        }
                    },
                    "customClassName": "header__user-name hide-xs"
                }
            }
        },
        {
            "baseProperties": {
                "name": "logout",
                "type": "staticIcon",
                "id": "logout"
            },
            "iconClass": "fas fa-sign-out-alt",
            "layoutProperties": {
                "css": {
                    "configuration": [{
                        "selectorName": ".header-common__logout",
                        "xs": {
                            "white-space": "nowrap",
                            "justify-content": "end",
                            "cursor": "pointer",
                            "display": "flex",
                            "height": "100%",
                            "align-items": "center"
                        }
                    },
                    {
                        "selectorName": ".header-common__logout .dynamic-style-child",
                        "xs": {
                            "font-size": "26px",
                            "color": "#0369FF",
                            "margin-right": "10px"
                        }
                    }],
                    "customClassName": "header-common__logout"
                }
            },
            "events": [
                {
                    "event": "click",
                    "actions": [
                        {
                            "actionType": "INVOKE_API",
                            "showLoading": true,
                            "apiConfig": {
                                "name": "signupButtonApi",
                                "url": "/rest/logout",
                                "method": "POST",
                                "payloadParams": [],
                                "onSuccess": {
                                    "pageUrl": "/login",
                                    "clearSharedData": [
                                        "authenticated",
                                        "name"
                                    ]
                                }
                            }
                        }
                    ],
                    "debounce": 3000
                }
            ]
        }
    ]
}