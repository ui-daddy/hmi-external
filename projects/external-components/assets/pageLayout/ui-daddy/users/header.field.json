            "layoutProperties": {
                "css": {
                    "configuration": [
                        {
                            "selectorName": ".header__org",
                            "xs": {
                                "justify-content": "end"
                            }
                        },
                        {
                            "selectorName": ".header__org .dynamic-style-child",
                            "xs": {
                                "color": "black",
