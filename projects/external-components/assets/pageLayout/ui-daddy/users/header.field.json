                                "font-weight": "bold",
                                "margin-top": "10px"
                            }
                        }
                    ],
                    "customClassName": "header__org"
                }
            }
        },
        {
            "value": "userId",
            "baseProperties": {
                "name": "userId",
                "type": "hidden",
                "id": "userIdHidden"
            },
            "dynamicValueAccessor": [
                "id"
            ],
            "layoutProperties": {}
        },
        {
            "baseProperties": {
                "name": "logout",
                "label": "Log Out",
                "type": "button",
                "id": "logout"
            },
            "layoutProperties": {
                "css": {
                    "configuration": [
                        {
                            "selectorName": ".header-common__logout",
                            "xs": {
                                "white-space": "nowrap",
                                "justify-content": "flex-end"
                            }
                        },
                        {
                            "selectorName": ".header-common__logout .dynamic-style-child",
                            "xs": {
                                "margin-top": "9px"
                            }
                        }
                    ],
                    "customClassName": "header-common__logout theme-btn",
                    "themeClass": "btn"
                }
            },
            "events": [
                {
                    "event": "click",
                    "actions": [
                        {
                            "actionType": "INVOKE_API",
                            "formName": "",
                            "apiConfig": {
                                "name": "signupButtonApi",
                                "url": "/api/v1/EmployeeAccount/Logoff",
                                "method": "POST",
                                "payloadParams": [
                                    {
                                        "paramName": "userId",
                                        "mappedField": "userId",
                                        "mappedValue": "",
                                        "valueFormat": "$SHARED_DATA(id)"
                                    }
                                ],
                                "onSuccess": {
                                    "pageUrl": "/login",
                                    "clearSharedData": [
                                        "name",
                                        "id",
                                        "givenName",
                                        "familyName",
                                        "authenticated"
                                    ]
                                }
                            }
                        }
                    ],
                    "debounce": 3000
                }
            ]
        }
    ],
